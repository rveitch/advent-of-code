{
  "root": true,
  "env": {
    "es2022": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "extends": [
    "airbnb-base",
    "plugin:import/recommended"
  ],
  "rules": {
      "import/extensions": ["error", "ignorePackages"],
      "max-len": "off",
      "no-use-before-define": ["error", { "functions": false }],
      "no-underscore-dangle": "off",
      "no-console": "off"
  }
}
